        "/api/events": {
            "get": {
                "tags": [
                    "Events"
                ],
                "summary": "GET events",
                "description": "Return data events yang akan ditampilkan pada halaman events mobile",
                "operationId": "02564acadf6f48e7cf453bb2319ac1b4",
                "parameters": [
                    {
                        "name": "type",
                        "in": "query",
                        "description": "Type of event",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "proker",
                                "kegiatan",
                                "lomba"
                            ]
                        }
                    },
                    {
                        "name": "date",
                        "in": "query",
                        "description": "Date of event",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date"
                        }
                    },
                    {
                        "name": "time",
                        "in": "query",
                        "description": "Time of event",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "time"
                        }
                    },
                    {
                        "name": "location",
                        "in": "query",
                        "description": "Location of event",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Search event by name",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sort",
                        "in": "query",
                        "description": "Sort event by date",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "asc",
                                "desc"
                            ]
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "Limit event",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page of event",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "403": {
                        "description": "Access Denied"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
